div(ng-repeat='alternative in alternatives')
        p Alternativ: {{alternative.description}}

.row
    .col-xs-12.col-sm-offset-3.col-sm-6.col-md-offset-4.col-md-4.text-center
        form.form-group(ng-submit='addAlternative(alternative)')
            label Alternative
            input.form-control(type='text', name='alternative', placeholder='Skriv inn alternativ', ng-model='alternative.title')
            br
            button#submit.btn(type='submit', value='Submit') Submit

p {{formFeedback}}
